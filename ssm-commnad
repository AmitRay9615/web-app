aws ssm send-command --region ap-southeast-1 --document-name 'AWS-RunShellScript' --comment 'checking web-server configuration' --parameters commands="cd /etc/nginx/sites-enabled; nginx -t; service nginx status" --targets Key=tag:Entity,Values=Entity --cloud-watch-output-config 'CloudWatchOutputEnabled=true,CloudWatchLogGroupName=crmdeploy_test' --query 'Command.CommandId' --output text
